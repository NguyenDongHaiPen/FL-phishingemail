{
  "manifest_version": 2,
  "name": "Phishing Detector",
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "phishing-detector@example.com"
    }
  },
  "permissions": [
    "messagesRead",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png"
    }
  }
}
